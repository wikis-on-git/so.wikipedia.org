  #switch: {{ lc: {{{type}}} }}
  | tv movie | tv film 
  | commercial | spot  
  | special
  | movie | film
  | movie series | film series
  | oav | ova | ona | oad
  | music | music video
  | live tv film
  | live video
  | live movie
  | live film = {{{runtime<includeonly>|</includeonly>}}}
}}

|label12  = Budget
|data12   = {{
  #switch: {{ lc: {{{type}}} }}
  | tv movie | tv film 
  | movie | film
  | movie series | film series
  | live tv film
  | live movie
  | live film = {{{budget<includeonly>|</includeonly>}}}
}}

|label13  = Box office
|data13   = {{
  #switch: {{ lc: {{{type}}} }}
  | movie | film
  | movie series | film series
  | live movie
  | live film = {{{gross<includeonly>|</includeonly>}}}
}}

