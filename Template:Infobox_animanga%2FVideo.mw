  #switch: {{ lc: {{{type}}} }}
  | movie series
  | film series  = Films
  | #default     = Episodes
}}
|data14   = {{
  #switch: {{ lc: {{{type}}} }}
  | tv  | series
  | tv series
  | drama
  | movie series | film series
  | oav | ova 
