|label10   = {{#if:{{{website|}}}|Website}}
|data10    = {{#if:{{{website|}}}
             | {{{website|}}}
             | {{#if:{{{url|}}} | {{{url|}}} }} }}

|header11  = {{ifempty|{{{embedded1|}}}|{{{embedded|}}} }}

|header12  = {{{embedded2|}}}

}}<noinclude>
